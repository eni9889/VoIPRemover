ARCHS = armv7 arm64
TARGET =: clang

include theos/makefiles/common.mk

BUNDLE_NAME = VoIPRemover
VoIPRemover_FILES = VoIPRemover.mm
VoIPRemover_INSTALL_PATH = /Library/PreferenceBundles
VoIPRemover_FRAMEWORKS = UIKit
VoIPRemover_PRIVATE_FRAMEWORKS = Preferences

include $(THEOS_MAKE_PATH)/bundle.mk

internal-stage::
	$(ECHO_NOTHING)mkdir -p $(THEOS_STAGING_DIR)/Library/PreferenceLoader/Preferences$(ECHO_END)
	$(ECHO_NOTHING)cp entry.plist $(THEOS_STAGING_DIR)/Library/PreferenceLoader/Preferences/VoIPRemover.plist$(ECHO_END)
